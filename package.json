{
  "name": "com.unity.package-manager.metadata",
  "version": "1.1.0",
  "description": "Contains metadata used by the com.unity.package-manager package to fulfill client requests",
  "devDependencies": {
    "semantic-release": "^17.2.3",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0"
  },
  "homepage": "https://github.com/mob-sakai/unity.package-manager.metadata",
  "publishConfig": {
    "registry": "http://localhost"
  },
  "files": [],
  "scripts": {
    "update": "semantic-release",
    "update:local": "./update_packages.sh && npm --no-git-tag-version version patch && npm publish",
    "update:debug": "semantic-release --no-ci --dry-run --debug"
  },
  "searchablePackages": []
}
